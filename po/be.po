#: libgnomekbd/org.gnome.libgnomekbd.keyboard.gschema.xml.in:7
msgid "keyboard model"
msgstr "мадэль клавіятуры"

#: libgnomekbd/org.gnome.libgnomekbd.keyboard.gschema.xml.in:11
